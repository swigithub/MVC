@using walkDirectory
@model IEnumerable<GetDirctoryinformation>
@if (Model.Count() > 0)
{
<table class="table directorytbl table-bordered" style="width:100%">
    <thead>
        <tr>
            <th></th>
            <th>Name</th>
            <th>Date</th>
            <th>Type</th>
            <th>Size in kb</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var item in Model)
    {
        <tr>
            <td><input type="radio" onclick="runAVXParser(this)" value="@item.directoryPath" name="name" /></td>
            <td>@item.fileName</td>
            <td>@item.createDate</td>
            <td>@item.fileType</td>
            <td>@item.size</td>
        </tr>
    }
    </tbody>
   

</table>
<script>
    function runAVXParser(e) {
        debugger;
        $.post("../Parser/GetFileFromWorkorder", { FilePath: $(e).val() }, function (data) {
            var url = "../Parser/Parse";
            window.open(url, "_blank");
        })
    }
</script>
}
