                                            </tr>
                                            <tr>
                                                <td style="background:#004B99;color:white">RACH Aborted incl. retries</td>
                                                <td style="text-align:center">@Math.Round(rachAbortPerc, 1)%</td>
                                                <td style="text-align:center">@Math.Round(rachAbortPerc, 1)%</td>
                                                <td></td>
                                                <td></td>
                                            </tr>
                                        </tbody>
                                    </table>
