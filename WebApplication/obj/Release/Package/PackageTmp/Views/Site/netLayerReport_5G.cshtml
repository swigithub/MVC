                                    <table class="report_table">
                                        <thead>
                                            <tr>
                                                <th></th>
                                                <th style="text-align:center" >All Tests</th>
                                                <th style="text-align:center">5G Only</th>
                                                <th style="text-align:center">LTE Only</th>
                                                <th style="text-align:center">Mixed Mode</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            @{
											    int totalCounts = 0;
                                                time5GLTE = Model.ToList()[0].AvgConSetupTime;
                                                totalCounts = Model.ToList()[0].SiteTestLog.Where(s => (s.StackTrace.ToUpper()).Contains("RACH_ATTEMPT")).Count();
                                                <!--rachFailureCount = Model.ToList()[0].SiteTestLog.Where(s => (s.StackTrace.ToUpper()).Contains("RACH_ATTEMPT:FAILURE")).Count()-->;
											
                                                rachFailMSG2Count = Model.ToList()[0].SiteTestLog.Where(s => (s.StackTrace.ToUpper()).Contains("RACH_ATTEMPT:FAILURE_MSG2")).Count();
                                                rachFailMSG4Count = Model.ToList()[0].SiteTestLog.Where(s => (s.StackTrace.ToUpper()).Contains("RACH_ATTEMPT:FAILURE_MSG4")).Count();
                                                <!--rachAbortedCount = Model.ToList()[0].SiteTestLog.Where(s => (s.StackTrace.ToUpper()).Contains("RACH_ATTEMPT:ABORTED")).Count();-->
                                                rachAbortedCount = Model.ToList()[0].SiteTestLog.Where(s => (s.StackTrace.ToUpper()).Contains("RACH_ATTEMPT:ABORTED") || s.StackTrace.ToUpper().Contains("RACH_ATTEMPT:INVALID")).Count();
                                                rachSuccessCount = Model.ToList()[0].SiteTestLog.Where(s => (s.StackTrace.ToUpper()).Contains("RACH_ATTEMPT:SUCCESS")).Count();
                                                rachFailureCount = totalCounts - rachSuccessCount;
                                                string prevNetMode = string.Empty;
                                                foreach (var item in Model.ToList()[0].SiteTestLog.Where(s=>s.StackTrace.Contains("EVENT")).ToList())
                                                {
                                                    if (item.StackTrace.Contains("LTE_AND_NR"))
                                                    {
                                                        if (prevNetMode=="LTE")
                                                        {
                                                            LTEtoNRCount++;
                                                        }
                                                    }
                                                    if (item.StackTrace.Contains("LTE:LTE"))
                                                    {
                                                        if (prevNetMode=="NR")
                                                        {
                                                            NRtoLTECount++;
                                                        }
                                                    }
                                                    var split = item.StackTrace.Split(':');
                                                    if (split.Any())
                                                    {
                                                        prevNetMode = split.First();
                                                    }
                                                }
                                                //NRtoLTECount = Model.ToList()[0].SiteTestLog.Where(s => (s.StackTrace.ToUpper()).Contains("NR:HO:SUCCESS:NR-LTE") || s.StackTrace.ToUpper().Contains("NR:HO:SUCCESS:LTE-NR")).Count();
                                                //LTEtoNRCount = Model.ToList()[0].SiteTestLog.Where(s => (s.StackTrace.ToUpper()).Contains("LTE:HO:SUCCESS:LTE-NR")).Count();
                                                var containRachAttemptSuccess = Model.ToList()[0].SiteTestLog.Where(s => (s.StackTrace.ToUpper()).Contains("RACH_ATTEMPT:SUCCESS"));
                                                var firstAttemptSuccess = containRachAttemptSuccess.Where(s => (s.StackTrace.ToUpper()).Contains("UL_GRANT:1"));
                                                <!--int totalCounts = 0;
                                                time5GLTE = Model.ToList()[0].AvgConSetupTime;
                                                totalCounts = rachFailureCount + rachSuccessCount + rachAbortedCount;-->
                                                if (totalCounts > 0)
                                                {
                                                    rachFailureRate = (rachFailureCount * 100) / totalCounts;
                                                    rachAbortPerc = (rachAbortedCount * 100) / totalCounts;
                                                    rachFailMSG2 = (rachFailMSG2Count * 100) / totalCounts;
                                                    rachFailMSG4 = (rachFailMSG4Count * 100) / totalCounts;
                                                }
                                                if (rachSuccessCount > 0)
                                                {
                                                    rachFirstAttempt = (firstAttemptSuccess.Count() * 100) / (containRachAttemptSuccess.Except(firstAttemptSuccess).Count()+firstAttemptSuccess.Count());
                                                    rachMultipleAttempt = (containRachAttemptSuccess.Except(firstAttemptSuccess).Count() * 100) / (containRachAttemptSuccess.Count()+firstAttemptSuccess.Count());
                                                }
                                            }
                                            <tr>
                                                <td style="background:#004B99;color:white">Time on LTE-5GNR</td>
                                                <td style="text-align:center">@time5GLTE%</td>
                                                <td style="text-align:center">@time5GLTE%</td>
                                                <td style="text-align:center"></td>
                                                <td style="text-align:center"></td>
                                            </tr>
                                            <tr>
                                                <td style="background:#004B99;color:white"># 5G to 4G Transitions</td>
                                                <td style="text-align:center">@NRtoLTECount</td>
                                                <td style="text-align:center">@NRtoLTECount</td>
                                                <td></td>
                                                <td></td>
                                            </tr>
                                            <tr>
                                                <td style="background:#004B99;color:white"># 4G to 5G Transitions</td>
                                                <td style="text-align:center">@LTEtoNRCount</td>
                                                <td style="text-align:center">@LTEtoNRCount</td>
                                                <td></td>
                                                <td></td>
                                            </tr>
                                            <tr>
                                                <td style="background:#004B99;color:white">RACH Failure Count (#)</td>
                                                <td style="text-align:center">@rachFailureCount</td>
                                                <td style="text-align:center">@rachFailureCount</td>
                                                <td></td>
                                                <td></td>
                                            </tr>
                                            <tr>
                                                <td style="background:#004B99;color:white">RACH Success Count (#)</td>
                                                <td style="text-align:center">@rachSuccessCount</td>
                                                <td style="text-align:center">@rachSuccessCount</td>
                                                <td></td>
                                                <td></td>
                                            </tr>
                                            <tr>
                                                <td style="background:#004B99;color:white">Overall RACH Failure Rate</td>
                                                <td style="text-align:center">@Math.Round(rachFailureRate, 1)%</td>
                                                <td style="text-align:center">@Math.Round(rachFailureRate, 1)%</td>
                                                <td></td>
                                                <td></td>
                                            </tr>
                                            <tr>
                                                <td style="background:#004B99;color:white">RACH OK in first attempt</td>
                                                <td style="text-align:center">@Math.Round(rachFirstAttempt, 1)%</td>
                                                <td style="text-align:center">@Math.Round(rachFirstAttempt, 1)%</td>
                                                <td></td>
                                                <td></td>
                                            </tr>
                                            <tr>
                                                <td style="background:#004B99;color:white">RACH OK in multiple attempts</td>
                                                <td style="text-align:center">@Math.Round(rachMultipleAttempt, 1)%</td>
                                                <td style="text-align:center">@Math.Round(rachMultipleAttempt, 1)%</td>
                                                <td></td>
                                                <td></td>
                                            </tr>
                                            <tr>
                                                <td style="background:#004B99;color:white">RACH Failure at MSG2 incl. retries</td>
                                                <td style="text-align:center">@Math.Round(rachFailMSG2, 1)%</td>
                                                <td style="text-align:center">@Math.Round(rachFailMSG2, 1)%</td>
                                                <td></td>
