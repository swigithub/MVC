@model IEnumerable <AirView.DBLayer.AirView.Entities.AD_Help>
@{
    ViewBag.Title = "ReadHelp";
    Layout = "~/Views/Shared/_LayoutHelp.cshtml";
}
<div id="display">
   @foreach (var item in Model)
   {
       <h2>@Html.DisplayFor(modelItem => item.Title)</h2>
       <div>@Html.Raw(item.Description)</div>
       <div><b>Bind To:</b> @Html.DisplayFor(modelItem => item.HelpId)</div>
    <p>
        @Html.ActionLink("Edit", "EditHelp/"+ item.HelpId) |
        @Html.ActionLink("Back to List", "/listhelp")
    </p>
   }
</div>
