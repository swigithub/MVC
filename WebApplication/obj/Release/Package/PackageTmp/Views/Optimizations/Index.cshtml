    <script>
        $('#isShow').hide();
        $('#isShow1').hide();
        $('#isShow2').hide();
        var layer = {};
        var data = {};

        $(document).on('click', '.BandRow', function () {
           
            var siteId = null;
            $('#isShow').show();
            $('#isShow1').show();
            $('#isShow2').show();
            $('.BandRow').css({ "background": "#f9f9f9", "color": "#000" });
            $(this).css({ "background": "blue", "color": "#fff" });
            layer.NetworkModeId = $(this).attr('data-networkmodeid');
            layer.CarrierId = $(this).attr('data-carrierid');
            layer.BandId = $(this).attr('data-bandid');
            layer.SiteId = $(this).attr('data-siteid');
