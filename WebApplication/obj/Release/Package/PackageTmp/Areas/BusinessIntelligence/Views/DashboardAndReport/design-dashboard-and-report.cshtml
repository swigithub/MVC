
<div class="overlay rmt" style="display:none;">
    <div class="overlay__inner">
        <div class="overlay__content">
            <span class="spinner"></span>
        </div>
    </div>
</div>
<div ng-controller="dashboardAndReportController as vm">
    <div class="new-template">

    </div>
    <a class="sidepanel-btn" ng-if="vm.coreModeTypeInte=='EDIT'" ng-show="vm.modeType=='edit'" ng-click="vm.resizeAllWidgetOnCrossButton()" href="#" title=""><i class="bar"></i></a>
    <div ng-if="vm.coreModeTypeInte=='EDIT'" class="side-panel" sidepanel ng-show="vm.modeType=='edit'">
        <ul class="nav">
            <li><a class="active penalSettings" data-toggle="tab" data-target="#tab1" title="">Settings</a></li>
            <li><a class="querybuilder" ng-click="vm.reloadingOfMurri()" data-toggle="tab" data-target="#queryBuilderSection" title="">Query Builder</a></li>
        </ul>
        <!-- <h3>Settings Panel</h3> -->

        <div class="tab-content">
            <div class="tab-pane fade show active" id="tab1">
                <div class="toggle scrollbar" aircodaccordion>
                    <div class="toggle-item">
                        <h2><i class="fa fa-columns"></i> Panels</h2>
                        <div class="content">
                            <div class="row narrow panels">
                                <div class="col-lg-4 col-md-12" ng-repeat="item in  vm.standardWidgets track by $index">
                                    <div class="panel-box draggable" data-type="{{item.type}}" title="{{item.title}}">
                                        <span><i class="{{item.icon}}"></i></span>
                                        <h5>{{item.title}}</h5>
                                    </div>
                                </div>


                            </div>
                        </div>
                    </div><!-- Toggle Item -->

                    <div class="toggle-item">
                        <h2><i class="fa fa-cog"></i>Page Settings</h2>
                        <div class="content">
                            <form>
                                <div class="row">
                                    <div class="col-lg-12 col-md-12">
                                        <label>Page Color</label>
                                        <input id="pagecolor" class="bgcolor" type="text" value="#fafafa" />
                                        <div class="colorpickerwrapper"><div colorpicker></div></div>
                                    </div>
                                    <div class="col-lg-12 col-md-12">
                                        <input id="contained" name="contained" type="checkbox" /> <label for="contained">Contained</label>
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>

                    <div class="toggle-item" ng-show="vm.dynamicPridefinedWidget.length>0">
                        <h2><i class="fa fa-columns"></i> Predefined Widgets</h2>
                        <div class="content">
                            <div class="row narrow panels">
                                <div class="col-lg-4 col-md-12" ng-repeat="item in  vm.dynamicPridefinedWidget track by $index ">
                                    <div class="panel-box draggable" data-type="{{item.type}}" core-data-type="{{item.coreType}}" title="{{item.title}}">
                                        <span><i class="{{item.icon}}"></i></span>
                                        <h5>{{item.title}}</h5>
                                    </div>
                                </div>

                            </div>
                        </div>
                    </div>

                </div>

            </div>
            <div class="tab-pane fade" id="queryBuilderSection">
            </div>
        </div>

    </div>
</div>
