<script>
    $(document).ready(function () {
        // Custom Accordions
        $(".acc-box:first-child").find('.acc-body').slideDown();
        $(".acc-title").on("click", function () {
            $(this).next('.acc-body').slideToggle();
            //$(this).parents('.acc-box').siblings('.acc-box').find('.acc-body').slideUp();
        });


        $("body").on("change", ".switch > input", function () {
            if ($(this).prop('checked') == true) {
                $(this).parents('.setting-field').find('.more-opts').addClass('active');
            }
            if ($(this).prop('checked') == false) {
                $(this).parents('.setting-field').find('.more-opts').removeClass('active');
            }
        });
        
        $("body").on("change", ".inbox-notification input", function () {
            if ($(this).prop('checked') == true) {
                $(this).parents('tr').find('.stop-actions').addClass('enable');
            }
            if ($(this).prop('checked') == false) {
                $(this).parents('tr').find('.stop-actions').removeClass('enable');
            }
        });

        

        //console.log('result:' + JSON.stringify(model));
 
        $.ajax({
            url: "http://localhost:18459/swi/GETAlertConfiguration",
            type: "POST",
            data: JSON.stringify({ "KeyCode": "" }),
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (result) {
                var TempDom = "";
                $.each(result, function (i,k) {
                    TempDom += k.AlertCategoryName + ' | ' + k.Name + ' | ' + '<label><input type="checkbox"> Subscribe</label> <label><input type="checkbox"> Push</label> <label><input type="checkbox"> Email</label><br>';
                });

                console.log('result:' + JSON.stringify(result));
                $('#PermissionArea').html(TempDom);
            }
        });

    });
</script>