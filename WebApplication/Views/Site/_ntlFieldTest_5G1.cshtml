    <script>
        var ReportConfiguration = JSON.parse('@Html.Raw(ReportConfiguration)');
        function Configurations(keyCode, dataType, value, IsActive) {
            debugger
            // console.log(keyCode + '-' + dataType + '-' + value + '-' + IsActive);
            if (dataType == 'BOOLEAN') {
                if (IsActive == true) {
                    $('#' + keyCode).show();
                    $('.' + keyCode).show();
                    $('#pan-' + keyCode).show();
                } else {
                    $('#' + keyCode).hide();
                    $('.' + keyCode).hide();
                    $('#pan-' + keyCode).hide();
                }
            }

            if (dataType == 'BGCOLOR') {
                debugger
                $('.' + keyCode).css({
                    'background': value
                });
            }

            if (dataType == 'STRING') {
                if (value != null || value != '') {
                    var preValue = $('#' + keyCode).attr('data-preValue');
                    if (preValue != undefined) {
                        $('#' + keyCode).text(preValue + ' ' + value);
                    } else {
                        $('#' + keyCode).text(value);
                    }

                    $('.' + keyCode).text(value);
                }
            }
        }

    </script>
