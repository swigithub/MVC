    var ccwCheckExist = Model.Where(a => a.Ccwhandoverstatus == null).Count();
    if (ccwCheckExist >= 3)
    {
        isCCW = false;
    }
    var cwCheckExist = Model.Where(a => a.CwHandoverStatus == null).Count();
    if (cwCheckExist >= 3)
    {
        isCW = false;
    }
    market = First.City;
    string NetwordMode = string.Empty;
