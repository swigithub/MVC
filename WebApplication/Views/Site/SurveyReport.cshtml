<script type="text/javascript">
    var btn_print = $('.print');


    btn_print.click(function () {

        btn_print.hide();

        setTimeout(function () {
            window.print();

            btn_print.show();
       
        }, 2000);




        return false;
    });
</script>

</body>
</html>







