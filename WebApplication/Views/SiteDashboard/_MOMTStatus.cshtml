@using SWI.Libraries.AirView.Entities
@model IEnumerable<MOMTStatus>

<div class="box box-primary">
    <div class="box-header">
        <h3 class="box-title">MO/MT Status</h3>
    </div>
    <!-- /.box-header -->
    <div class="box-body no-padding">
        <table class="table table-striped" style="text-align:center">
            <thead>
                <tr>
                    <th>Sector</th>

                    <th style="width: 10px">eCSFB MO</th>
                    <th>eCSFB MT</th>
                    <th>VoLTE MO</th>
                    <th style="width: 40px">VoLTE MT</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@item.Sector</td>
                        <td>
                            @if (item.MoStatus)
                            {
                                <i style="color:green" class="fa fa-check"></i>
                            }
                            else if (!item.MoStatus == false)
                            {
                                <i style="color:red" class="fa fa-close"></i>
                            }
                        </td>
                        <td>
                            @if (item.MtStatus)
                            {
                                <i style="color:green" class="fa fa-check"></i>
                            }
                            else if (!item.MtStatus == false)
                            {
                                <i style="color:red" class="fa fa-close"></i>
                            }
                        </td>
                        <td>
                           @if (item.VMoStatus)
                           {
                            <i style="color:green" class="fa fa-check"></i>
                           }
                           else if (!item.VMoStatus == false)
                           {
                            <i style="color:red" class="fa fa-close"></i>
                           }
                        </td>
                        <td>
                            @if (item.VMtStatus)
                            {
                                <i style="color:green" class="fa fa-check"></i>
                            }
                            else if (!item.VMtStatus == false)
                            {
                                <i style="color:red" class="fa fa-close"></i>
                            }
                        </td>
                    </tr>

                }
             
            </tbody>
        </table>
    </div>
    <!-- /.box-body -->
</div>
